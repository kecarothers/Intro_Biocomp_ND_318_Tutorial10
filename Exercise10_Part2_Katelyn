#Write custom function for SIR model
def SIR(y,t0,B,g): #B=beta g=gamma b/c gamma symbol looks a lot like y 
    S=y[0]
    I=y[1]
    dSdt=-BIS
    dIdt=BIS-gI
    dRdt=gI 
    return[dSdt,dIdt,dRdt]
